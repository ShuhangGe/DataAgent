# OpenAI Configuration (Latest Models)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o  # Latest OpenAI model
OPENAI_TEMPERATURE=0.1

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/sekai_analytics

# Data Sources
DATA_WAREHOUSE_URL=your_data_warehouse_url
VECTOR_DB_URL=your_vector_db_url

# Sekai Product Context
SEKAI_PRODUCT_DOMAIN=gaming  # or entertainment, social, etc.
SEKAI_TIMEZONE=Asia/Tokyo

# Analysis Configuration
MAX_RETRIES=3
CHUNK_SIZE=10000
SAMPLE_SIZE=100000

# Monitoring & Logging
LOG_LEVEL=INFO
ENABLE_MONITORING=true 